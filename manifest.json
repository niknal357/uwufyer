{
    "manifest_version": 3,
    "name": "uwufyer",
    "icons": {
        "256": "icon.png"
    },
    "description": "uwufys text",
    "version": "0.1",
    "permissions": [
        "storage",
        "tabs"
    ],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "toggle uwufyer"
    },
    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "js": [
            "content.js"
        ],
        "run_at": "document_end"
    }]
}